{
  "schemaVersion": 1,
  "id": "annoyancefix",
  "version": "${version}",

  "name": "AnnoyanceFix",
  "description": "Fixes annoyances and backports useful features.",
  "authors": [
    "Telvarost",
    "2zqa"
  ],
  "contact": {
    "issues": "https://github.com/telvarost/AnnoyanceFix-StationAPI/issues",
    "homepage": "https://github.com/telvarost/AnnoyanceFix-StationAPI/",
    "sources": "https://github.com/telvarost/AnnoyanceFix-StationAPI/"
  },

  "license": "CC0-1.0",
  "icon": "assets/annoyancefix/icon.png",

  "environment": "*",
  "entrypoints": {
    "stationapi:event_bus": [
      "net.glasslauncher.annoyancefix.events.init.RecipeListener",
      "net.glasslauncher.annoyancefix.events.ItemUsedInCraftingListener"
    ],
    "stationapi:event_bus_client": [
    ],
    "stationapi:event_bus_server": [
    ],
    "gcapi": [
      "net.glasslauncher.annoyancefix.Config"
    ]
  },
  "mixins": [
    "annoyancefix.mixins.json"
  ],

  "depends": {
    "minecraft": "1.0.0-beta.7.3"
  }
}
